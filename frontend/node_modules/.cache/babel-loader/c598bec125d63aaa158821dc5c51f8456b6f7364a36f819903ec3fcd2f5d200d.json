{"ast":null,"code":"import { professionalAPI } from '@/services/api';\nexport default {\n  name: 'ProfessionalProfile',\n  data() {\n    return {\n      profile: null,\n      loading: false,\n      error: null,\n      editMode: false,\n      form: {\n        description: '',\n        phone: '',\n        experience: '',\n        availability: []\n      }\n    };\n  },\n  created() {\n    this.fetchProfile();\n  },\n  methods: {\n    async fetchProfile() {\n      this.loading = true;\n      try {\n        // Fetch profile data\n        const data = await professionalAPI.getProfile();\n        this.profile = data;\n\n        // Fetch reviews if not included in profile data\n        if (!this.profile.reviews) {\n          const reviews = await professionalAPI.getReviews();\n          this.profile.reviews = reviews || [];\n        }\n        this.form = {\n          description: data.description || '',\n          phone: data.phone || '',\n          experience: data.experience || '',\n          availability: data.availability || []\n        };\n      } catch (err) {\n        this.error = err.message || 'Failed to load profile';\n        console.error('Error fetching profile:', err);\n      } finally {\n        this.loading = false;\n      }\n    },\n    async updateProfile() {\n      this.loading = true;\n      try {\n        await professionalAPI.updateProfile(this.form);\n        this.profile = {\n          ...this.profile,\n          ...this.form\n        };\n        this.editMode = false;\n        this.$toast?.success('Profile updated successfully') || alert('Profile updated successfully');\n      } catch (err) {\n        this.$toast?.error(err.message || 'Failed to update profile') || alert('Failed to update profile: ' + (err.message || ''));\n        console.error('Error updating profile:', err);\n      } finally {\n        this.loading = false;\n      }\n    },\n    cancelEdit() {\n      this.editMode = false;\n      this.form = {\n        description: this.profile.description || '',\n        phone: this.profile.phone || '',\n        experience: this.profile.experience || '',\n        availability: this.profile.availability || []\n      };\n    },\n    formatDate(dateString) {\n      if (!dateString) return 'N/A';\n      const date = new Date(dateString);\n      return date.toLocaleString('en-US', {\n        year: 'numeric',\n        month: 'short',\n        day: 'numeric'\n      });\n    }\n  }\n};","map":{"version":3,"names":["professionalAPI","name","data","profile","loading","error","editMode","form","description","phone","experience","availability","created","fetchProfile","methods","getProfile","reviews","getReviews","err","message","console","updateProfile","$toast","success","alert","cancelEdit","formatDate","dateString","date","Date","toLocaleString","year","month","day"],"sources":["src/views/ProfessionalProfile.vue"],"sourcesContent":["<template>\r\n  <div class=\"professional-profile-page\">\r\n    <div class=\"container py-5\">\r\n      <!-- Loading state -->\r\n      <div v-if=\"loading\" class=\"text-center my-5\">\r\n        <div class=\"spinner-border text-primary\" role=\"status\">\r\n          <span class=\"visually-hidden\">Loading...</span>\r\n        </div>\r\n        <p class=\"mt-2\">Loading professional profile...</p>\r\n      </div>\r\n      \r\n      <!-- Error state -->\r\n      <div v-else-if=\"error\" class=\"alert alert-danger\">\r\n        <i class=\"fas fa-exclamation-triangle me-2\"></i>\r\n        {{ error }}\r\n      </div>\r\n      \r\n      <!-- Content when loaded -->\r\n      <div v-else class=\"row\">\r\n        <!-- Profile Information -->\r\n        <div class=\"col-lg-4 mb-4\">\r\n          <div class=\"card profile-card\">\r\n            <div class=\"card-body text-center\">\r\n              <div class=\"profile-image mb-3\">\r\n                <img src=\"https://via.placeholder.com/150\" alt=\"Professional\" class=\"rounded-circle img-thumbnail\">\r\n              </div>\r\n              <h3>{{ profile.professional_name }}</h3>\r\n              \r\n              <!-- Edit Profile Section -->\r\n              <div v-if=\"editMode\">\r\n                <form @submit.prevent=\"updateProfile\">\r\n                  <div class=\"form-group mb-3\">\r\n                    <label>Description</label>\r\n                    <textarea \r\n                      v-model=\"form.description\" \r\n                      class=\"form-control\" \r\n                      rows=\"3\"\r\n                    ></textarea>\r\n                  </div>\r\n\r\n                  <div class=\"form-group mb-3\">\r\n                    <label>Phone</label>\r\n                    <input \r\n                      v-model=\"form.phone\" \r\n                      type=\"tel\" \r\n                      class=\"form-control\"\r\n                    >\r\n                  </div>\r\n\r\n                  <div class=\"form-group mb-3\">\r\n                    <label>Experience (years)</label>\r\n                    <input \r\n                      v-model=\"form.experience\" \r\n                      type=\"number\" \r\n                      class=\"form-control\"\r\n                      min=\"0\"\r\n                    >\r\n                  </div>\r\n\r\n                  <div class=\"btn-group w-100\">\r\n                    <button \r\n                      type=\"submit\" \r\n                      class=\"btn btn-primary\"\r\n                      :disabled=\"loading\"\r\n                    >\r\n                      Save Changes\r\n                    </button>\r\n                    <button \r\n                      type=\"button\" \r\n                      class=\"btn btn-secondary\"\r\n                      @click=\"cancelEdit\"\r\n                      :disabled=\"loading\"\r\n                    >\r\n                      Cancel\r\n                    </button>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n\r\n              <!-- View Profile Section -->\r\n              <div v-else>\r\n                <p class=\"text-muted\">{{ profile.description || 'No description available.' }}</p>\r\n                <hr>\r\n                <div class=\"text-start\">\r\n                  <p class=\"mb-2\"><strong><i class=\"fas fa-briefcase me-2\"></i>Experience:</strong> {{ profile.experience }} years</p>\r\n                  <p class=\"mb-2\"><strong><i class=\"fas fa-phone me-2\"></i>Phone:</strong> {{ profile.phone || 'Not provided' }}</p>\r\n                  <p class=\"mb-2\"><strong><i class=\"fas fa-calendar-check me-2\"></i>Joined:</strong> {{ formatDate(profile.date_created) }}</p>\r\n                  <button \r\n                    @click=\"editMode = true\" \r\n                    class=\"btn btn-primary mt-3 w-100\"\r\n                  >\r\n                    Edit Profile\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Reviews Section -->\r\n        <div class=\"col-lg-8\">\r\n          <div class=\"card\">\r\n            <div class=\"card-header bg-white\">\r\n              <h4 class=\"mb-0\">Reviews</h4>\r\n            </div>\r\n            <div class=\"card-body\">\r\n              <!-- Reviews content here -->\r\n              <div v-if=\"!profile.reviews || profile.reviews.length === 0\" class=\"text-center py-4\">\r\n                <p class=\"text-muted\">No reviews yet</p>\r\n              </div>\r\n              <div v-else>\r\n                <!-- Reviews list here -->\r\n                <div class=\"list-group list-group-flush\">\r\n                  <div v-for=\"review in profile.reviews\" :key=\"review.id\" class=\"list-group-item\">\r\n                    <div class=\"d-flex justify-content-between align-items-start mb-2\">\r\n                      <div>\r\n                        <h6 class=\"mb-0\">{{ review.customer_name || 'Customer' }}</h6>\r\n                        <small class=\"text-muted\">{{ formatDate(review.date_created) }}</small>\r\n                      </div>\r\n                      <div class=\"rating\">\r\n                        <i v-for=\"i in 5\" :key=\"i\" class=\"fas fa-star\" \r\n                          :class=\"i <= review.rating ? 'text-warning' : 'text-muted'\"></i>\r\n                      </div>\r\n                    </div>\r\n                    <p class=\"mb-0\">{{ review.remarks }}</p>\r\n                    <small class=\"text-muted\">Service: {{ review.service_name }}</small>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          \r\n          <!-- Service Statistics -->\r\n          <div class=\"card mt-4\">\r\n            <div class=\"card-header bg-white\">\r\n              <h4 class=\"mb-0\">Service Statistics</h4>\r\n            </div>\r\n            <div class=\"card-body\">\r\n              <div class=\"row g-4\">\r\n                <div class=\"col-sm-4\">\r\n                  <div class=\"stat-card bg-light p-3 text-center rounded\">\r\n                    <div class=\"stat-icon mb-2\">\r\n                      <i class=\"fas fa-check-circle fa-2x text-success\"></i>\r\n                    </div>\r\n                    <h5>{{ profile.completed_services || 0 }}</h5>\r\n                    <p class=\"text-muted mb-0\">Services Completed</p>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-4\">\r\n                  <div class=\"stat-card bg-light p-3 text-center rounded\">\r\n                    <div class=\"stat-icon mb-2\">\r\n                      <i class=\"fas fa-star fa-2x text-warning\"></i>\r\n                    </div>\r\n                    <h5>{{ profile.average_rating?.toFixed(1) || '0.0' }}</h5>\r\n                    <p class=\"text-muted mb-0\">Average Rating</p>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-sm-4\">\r\n                  <div class=\"stat-card bg-light p-3 text-center rounded\">\r\n                    <div class=\"stat-icon mb-2\">\r\n                      <i class=\"fas fa-users fa-2x text-primary\"></i>\r\n                    </div>\r\n                    <h5>{{ profile.total_customers || 0 }}</h5>\r\n                    <p class=\"text-muted mb-0\">Customers Served</p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { professionalAPI } from '@/services/api'\r\n\r\nexport default {\r\n  name: 'ProfessionalProfile',\r\n  \r\n  data() {\r\n    return {\r\n      profile: null,\r\n      loading: false,\r\n      error: null,\r\n      editMode: false,\r\n      form: {\r\n        description: '',\r\n        phone: '',\r\n        experience: '',\r\n        availability: []\r\n      }\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.fetchProfile()\r\n  },\r\n\r\n  methods: {\r\n    async fetchProfile() {\r\n      this.loading = true\r\n      try {\r\n        // Fetch profile data\r\n        const data = await professionalAPI.getProfile()\r\n        this.profile = data\r\n        \r\n        // Fetch reviews if not included in profile data\r\n        if (!this.profile.reviews) {\r\n          const reviews = await professionalAPI.getReviews()\r\n          this.profile.reviews = reviews || []\r\n        }\r\n        \r\n        this.form = {\r\n          description: data.description || '',\r\n          phone: data.phone || '',\r\n          experience: data.experience || '',\r\n          availability: data.availability || []\r\n        }\r\n      } catch (err) {\r\n        this.error = err.message || 'Failed to load profile'\r\n        console.error('Error fetching profile:', err)\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    },\r\n\r\n    async updateProfile() {\r\n      this.loading = true\r\n      try {\r\n        await professionalAPI.updateProfile(this.form)\r\n        this.profile = { ...this.profile, ...this.form }\r\n        this.editMode = false\r\n        this.$toast?.success('Profile updated successfully') || alert('Profile updated successfully')\r\n      } catch (err) {\r\n        this.$toast?.error(err.message || 'Failed to update profile') || alert('Failed to update profile: ' + (err.message || ''))\r\n        console.error('Error updating profile:', err)\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    },\r\n\r\n    cancelEdit() {\r\n      this.editMode = false\r\n      this.form = {\r\n        description: this.profile.description || '',\r\n        phone: this.profile.phone || '',\r\n        experience: this.profile.experience || '',\r\n        availability: this.profile.availability || []\r\n      }\r\n    },\r\n    \r\n    formatDate(dateString) {\r\n      if (!dateString) return 'N/A'\r\n      const date = new Date(dateString)\r\n      return date.toLocaleString('en-US', {\r\n        year: 'numeric',\r\n        month: 'short',\r\n        day: 'numeric'\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.professional-profile-page {\r\n  background-color: #f8f9fa;\r\n  min-height: 100vh;\r\n}\r\n\r\n.profile-card {\r\n  border-radius: 15px;\r\n}\r\n\r\n.profile-image {\r\n  width: 150px;\r\n  height: 150px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.profile-image img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n\r\n.rating {\r\n  display: inline-block;\r\n}\r\n\r\n.rating .fas {\r\n  color: #e0e0e0;\r\n}\r\n\r\n.rating .fas.text-warning {\r\n  color: #ffc107;\r\n}\r\n\r\n.list-group-item {\r\n  transition: background-color 0.2s;\r\n}\r\n\r\n.list-group-item:hover {\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.availability-grid {\r\n  display: flex;\r\n  gap: 10px;\r\n  overflow-x: auto;\r\n}\r\n\r\n.day-column {\r\n  flex: 1;\r\n  min-width: 100px;\r\n  text-align: center;\r\n  border: 1px solid #e0e0e0;\r\n  border-radius: 5px;\r\n  overflow: hidden;\r\n}\r\n\r\n.day-header {\r\n  background-color: #f8f9fa;\r\n  padding: 8px;\r\n  font-weight: 500;\r\n}\r\n\r\n.day-availability {\r\n  padding: 15px 8px;\r\n}\r\n\r\n.day-availability.is-available {\r\n  background-color: #d4edda;\r\n  color: #155724;\r\n}\r\n\r\n.day-availability:not(.is-available) {\r\n  background-color: #f8d7da;\r\n  color: #721c24;\r\n}\r\n\r\n.stat-card {\r\n  transition: transform 0.2s;\r\n  border-radius: 10px;\r\n}\r\n\r\n.stat-card:hover {\r\n  transform: translateY(-5px);\r\n  box-shadow: 0 0.5rem 1rem rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.stat-icon {\r\n  color: #6c757d;\r\n}\r\n</style>\r\n"],"mappings":"AAgLA,SAAAA,eAAA;AAEA;EACAC,IAAA;EAEAC,KAAA;IACA;MACAC,OAAA;MACAC,OAAA;MACAC,KAAA;MACAC,QAAA;MACAC,IAAA;QACAC,WAAA;QACAC,KAAA;QACAC,UAAA;QACAC,YAAA;MACA;IACA;EACA;EAEAC,QAAA;IACA,KAAAC,YAAA;EACA;EAEAC,OAAA;IACA,MAAAD,aAAA;MACA,KAAAT,OAAA;MACA;QACA;QACA,MAAAF,IAAA,SAAAF,eAAA,CAAAe,UAAA;QACA,KAAAZ,OAAA,GAAAD,IAAA;;QAEA;QACA,UAAAC,OAAA,CAAAa,OAAA;UACA,MAAAA,OAAA,SAAAhB,eAAA,CAAAiB,UAAA;UACA,KAAAd,OAAA,CAAAa,OAAA,GAAAA,OAAA;QACA;QAEA,KAAAT,IAAA;UACAC,WAAA,EAAAN,IAAA,CAAAM,WAAA;UACAC,KAAA,EAAAP,IAAA,CAAAO,KAAA;UACAC,UAAA,EAAAR,IAAA,CAAAQ,UAAA;UACAC,YAAA,EAAAT,IAAA,CAAAS,YAAA;QACA;MACA,SAAAO,GAAA;QACA,KAAAb,KAAA,GAAAa,GAAA,CAAAC,OAAA;QACAC,OAAA,CAAAf,KAAA,4BAAAa,GAAA;MACA;QACA,KAAAd,OAAA;MACA;IACA;IAEA,MAAAiB,cAAA;MACA,KAAAjB,OAAA;MACA;QACA,MAAAJ,eAAA,CAAAqB,aAAA,MAAAd,IAAA;QACA,KAAAJ,OAAA;UAAA,QAAAA,OAAA;UAAA,QAAAI;QAAA;QACA,KAAAD,QAAA;QACA,KAAAgB,MAAA,EAAAC,OAAA,oCAAAC,KAAA;MACA,SAAAN,GAAA;QACA,KAAAI,MAAA,EAAAjB,KAAA,CAAAa,GAAA,CAAAC,OAAA,mCAAAK,KAAA,iCAAAN,GAAA,CAAAC,OAAA;QACAC,OAAA,CAAAf,KAAA,4BAAAa,GAAA;MACA;QACA,KAAAd,OAAA;MACA;IACA;IAEAqB,WAAA;MACA,KAAAnB,QAAA;MACA,KAAAC,IAAA;QACAC,WAAA,OAAAL,OAAA,CAAAK,WAAA;QACAC,KAAA,OAAAN,OAAA,CAAAM,KAAA;QACAC,UAAA,OAAAP,OAAA,CAAAO,UAAA;QACAC,YAAA,OAAAR,OAAA,CAAAQ,YAAA;MACA;IACA;IAEAe,WAAAC,UAAA;MACA,KAAAA,UAAA;MACA,MAAAC,IAAA,OAAAC,IAAA,CAAAF,UAAA;MACA,OAAAC,IAAA,CAAAE,cAAA;QACAC,IAAA;QACAC,KAAA;QACAC,GAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}