{"ast":null,"code":"import { professionalAPI } from '@/services/api';\nexport default {\n  name: 'ProfessionalProfile',\n  data() {\n    return {\n      profile: null,\n      loading: false,\n      error: null,\n      editMode: false,\n      form: {\n        description: '',\n        phone: '',\n        experience: '',\n        availability: []\n      }\n    };\n  },\n  created() {\n    this.fetchProfile();\n  },\n  methods: {\n    async fetchProfile() {\n      this.loading = true;\n      try {\n        const data = await professionalAPI.getProfile();\n        this.profile = data;\n        this.form = {\n          description: data.description || '',\n          phone: data.phone || '',\n          experience: data.experience || '',\n          availability: data.availability || []\n        };\n      } catch (err) {\n        this.error = err.message || 'Failed to load profile';\n        console.error('Error fetching profile:', err);\n      } finally {\n        this.loading = false;\n      }\n    },\n    async updateProfile() {\n      this.loading = true;\n      try {\n        await professionalAPI.updateProfile(this.form);\n        this.profile = {\n          ...this.profile,\n          ...this.form\n        };\n        this.editMode = false;\n        this.$toast.success('Profile updated successfully');\n      } catch (err) {\n        this.$toast.error(err.message || 'Failed to update profile');\n        console.error('Error updating profile:', err);\n      } finally {\n        this.loading = false;\n      }\n    },\n    cancelEdit() {\n      this.editMode = false;\n      this.form = {\n        description: this.profile.description || '',\n        phone: this.profile.phone || '',\n        experience: this.profile.experience || '',\n        availability: this.profile.availability || []\n      };\n    }\n  }\n};","map":{"version":3,"names":["professionalAPI","name","data","profile","loading","error","editMode","form","description","phone","experience","availability","created","fetchProfile","methods","getProfile","err","message","console","updateProfile","$toast","success","cancelEdit"],"sources":["src/views/ProfessionalProfile.vue"],"sourcesContent":["<template>\r\n  <div class=\"professional-profile-page\">\r\n    <div class=\"container py-5\">\r\n      <!-- Loading state -->\r\n      <div v-if=\"loading\" class=\"text-center my-5\">\r\n        <div class=\"spinner-border text-primary\" role=\"status\">\r\n          <span class=\"visually-hidden\">Loading...</span>\r\n        </div>\r\n        <p class=\"mt-2\">Loading professional profile...</p>\r\n      </div>\r\n      \r\n      <!-- Error state -->\r\n      <div v-else-if=\"error\" class=\"alert alert-danger\">\r\n        <i class=\"fas fa-exclamation-triangle me-2\"></i>\r\n        {{ error }}\r\n      </div>\r\n      \r\n      <!-- Content when loaded -->\r\n      <div v-else class=\"row\">\r\n        <!-- Profile Information -->\r\n        <div class=\"col-lg-4 mb-4\">\r\n          <div class=\"card profile-card\">\r\n            <!-- Profile Card Content -->\r\n            <div class=\"card-body text-center\">\r\n              <div class=\"profile-image mb-3\">\r\n                <img src=\"https://via.placeholder.com/150\" alt=\"Professional\" class=\"rounded-circle img-thumbnail\">\r\n              </div>\r\n              <h3>{{ profile.professional_name }}</h3>\r\n              <div class=\"d-flex justify-content-center mb-3\">\r\n                <div class=\"rating\">\r\n                  <i v-for=\"n in 5\" :key=\"n\" class=\"fas fa-star\" \r\n                    :class=\"n <= profile.average_rating ? 'text-warning' : 'text-muted'\"></i>\r\n                  <span class=\"ms-1\">{{ profile.average_rating }}/5 ({{ profile.total_reviews }} reviews)</span>\r\n                </div>\r\n              </div>\r\n              \r\n              <!-- Edit Profile Section -->\r\n              <div v-if=\"editMode\">\r\n                <form @submit.prevent=\"updateProfile\">\r\n                  <div class=\"form-group mb-3\">\r\n                    <label>Description</label>\r\n                    <textarea \r\n                      v-model=\"form.description\" \r\n                      class=\"form-control\" \r\n                      rows=\"3\"\r\n                    ></textarea>\r\n                  </div>\r\n\r\n                  <div class=\"form-group mb-3\">\r\n                    <label>Phone</label>\r\n                    <input \r\n                      v-model=\"form.phone\" \r\n                      type=\"tel\" \r\n                      class=\"form-control\"\r\n                    >\r\n                  </div>\r\n\r\n                  <div class=\"form-group mb-3\">\r\n                    <label>Experience (years)</label>\r\n                    <input \r\n                      v-model=\"form.experience\" \r\n                      type=\"number\" \r\n                      class=\"form-control\"\r\n                      min=\"0\"\r\n                    >\r\n                  </div>\r\n\r\n                  <div class=\"btn-group\">\r\n                    <button \r\n                      type=\"submit\" \r\n                      class=\"btn btn-primary\"\r\n                      :disabled=\"loading\"\r\n                    >\r\n                      Save Changes\r\n                    </button>\r\n                    <button \r\n                      type=\"button\" \r\n                      class=\"btn btn-secondary\"\r\n                      @click=\"cancelEdit\"\r\n                      :disabled=\"loading\"\r\n                    >\r\n                      Cancel\r\n                    </button>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n\r\n              <!-- View Profile Section -->\r\n              <div v-else>\r\n                <p class=\"text-muted\">{{ profile.description || 'No description available.' }}</p>\r\n                <hr>\r\n                <div class=\"text-start\">\r\n                  <p class=\"mb-2\"><strong><i class=\"fas fa-briefcase me-2\"></i>Experience:</strong> {{ profile.experience }} years</p>\r\n                  <p class=\"mb-2\"><strong><i class=\"fas fa-phone me-2\"></i>Phone:</strong> {{ profile.phone || 'Not provided' }}</p>\r\n                  <p class=\"mb-2\"><strong><i class=\"fas fa-calendar-check me-2\"></i>Joined:</strong> {{ formatDate(profile.date_created) }}</p>\r\n                  <button \r\n                    @click=\"editMode = true\" \r\n                    class=\"btn btn-primary mt-3\"\r\n                  >\r\n                    Edit Profile\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        \r\n        <!-- Reviews Section -->\r\n        <div class=\"col-lg-8\">\r\n          <div class=\"card\">\r\n            <div class=\"card-header bg-white\">\r\n              <h4 class=\"mb-0\">Customer Reviews</h4>\r\n            </div>\r\n            <div class=\"card-body p-0\">\r\n              <div v-if=\"reviews.length === 0\" class=\"text-center py-5\">\r\n                <i class=\"fas fa-star fa-3x text-muted mb-3\"></i>\r\n                <h5>No Reviews Yet</h5>\r\n                <p class=\"text-muted\">This professional hasn't received any reviews yet.</p>\r\n              </div>\r\n              \r\n              <div v-else class=\"list-group list-group-flush\">\r\n                <div v-for=\"review in reviews\" :key=\"review.id\" class=\"list-group-item p-3\">\r\n                  <div class=\"d-flex justify-content-between mb-2\">\r\n                    <div>\r\n                      <h5 class=\"mb-0\">{{ review.customer_name }}</h5>\r\n                      <small class=\"text-muted\">{{ formatDate(review.date_created) }}</small>\r\n                    </div>\r\n                    <div class=\"rating\">\r\n                      <i v-for=\"n in 5\" :key=\"n\" class=\"fas fa-star\" \r\n                        :class=\"n <= review.rating ? 'text-warning' : 'text-muted'\"></i>\r\n                    </div>\r\n                  </div>\r\n                  <p class=\"mb-1\">{{ review.remarks }}</p>\r\n                  <small class=\"text-muted\">Service: {{ review.service_name }}</small>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          \r\n          <!-- Availability Calendar (for demonstration) -->\r\n          <div class=\"card mt-4\">\r\n            <div class=\"card-header bg-white d-flex justify-content-between align-items-center\">\r\n              <h4 class=\"mb-0\">Availability</h4>\r\n              <div>\r\n                <button class=\"btn btn-outline-primary btn-sm\" @click=\"requestService\" :disabled=\"!profile.is_available\">\r\n                  <i class=\"fas fa-calendar-plus me-1\"></i> Request Service\r\n                </button>\r\n              </div>\r\n            </div>\r\n            <div class=\"card-body\">\r\n              <div v-if=\"!profile.is_available\" class=\"alert alert-warning\">\r\n                <i class=\"fas fa-exclamation-circle me-2\"></i>\r\n                This professional is currently unavailable for new service requests.\r\n              </div>\r\n              \r\n              <!-- Simple weekly availability display -->\r\n              <div class=\"availability-grid\">\r\n                <div v-for=\"day in availabilityCalendar\" :key=\"day.name\" class=\"day-column\">\r\n                  <div class=\"day-header\">{{ day.name }}</div>\r\n                  <div class=\"day-availability\" :class=\"{ 'is-available': day.available }\">\r\n                    {{ day.available ? 'Available' : 'Unavailable' }}\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"professional-profile container\">\r\n    <h2 class=\"mb-4\">Professional Profile</h2>\r\n\r\n    <!-- Loading State -->\r\n    <div v-if=\"loading\" class=\"text-center\">\r\n      <div class=\"spinner-border\" role=\"status\">\r\n        <span class=\"sr-only\">Loading...</span>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Error State -->\r\n    <div v-if=\"error\" class=\"alert alert-danger\">\r\n      {{ error }}\r\n    </div>\r\n\r\n    <!-- Profile Content -->\r\n    <div v-if=\"profile && !loading\" class=\"card\">\r\n      <div class=\"card-body\">\r\n        <div class=\"d-flex justify-content-between align-items-center mb-4\">\r\n          <h3 class=\"card-title\">{{ profile.name }}</h3>\r\n          <button \r\n            v-if=\"!editMode\" \r\n            @click=\"editMode = true\" \r\n            class=\"btn btn-primary\"\r\n          >\r\n            Edit Profile\r\n          </button>\r\n        </div>\r\n\r\n        <!-- View Mode -->\r\n        <div v-if=\"!editMode\">\r\n          <p><strong>Description:</strong> {{ profile.description || 'No description provided' }}</p>\r\n          <p><strong>Phone:</strong> {{ profile.phone || 'No phone number provided' }}</p>\r\n          <p><strong>Experience:</strong> {{ profile.experience || 'No experience listed' }}</p>\r\n        </div>\r\n\r\n        <!-- Edit Mode -->\r\n        <form v-else @submit.prevent=\"updateProfile\">\r\n          <div class=\"form-group mb-3\">\r\n            <label>Description</label>\r\n            <textarea \r\n              v-model=\"form.description\" \r\n              class=\"form-control\" \r\n              rows=\"3\"\r\n            ></textarea>\r\n          </div>\r\n\r\n          <div class=\"form-group mb-3\">\r\n            <label>Phone</label>\r\n            <input \r\n              v-model=\"form.phone\" \r\n              type=\"tel\" \r\n              class=\"form-control\"\r\n            >\r\n          </div>\r\n\r\n          <div class=\"form-group mb-3\">\r\n            <label>Experience (years)</label>\r\n            <input \r\n              v-model=\"form.experience\" \r\n              type=\"number\" \r\n              class=\"form-control\"\r\n              min=\"0\"\r\n            >\r\n          </div>\r\n\r\n          <div class=\"btn-group\">\r\n            <button \r\n              type=\"submit\" \r\n              class=\"btn btn-primary\"\r\n              :disabled=\"loading\"\r\n            >\r\n              Save Changes\r\n            </button>\r\n            <button \r\n              type=\"button\" \r\n              class=\"btn btn-secondary\"\r\n              @click=\"cancelEdit\"\r\n              :disabled=\"loading\"\r\n            >\r\n              Cancel\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { professionalAPI } from '@/services/api'\r\n\r\nexport default {\r\n  name: 'ProfessionalProfile',\r\n  \r\n  data() {\r\n    return {\r\n      profile: null,\r\n      loading: false,\r\n      error: null,\r\n      editMode: false,\r\n      form: {\r\n        description: '',\r\n        phone: '',\r\n        experience: '',\r\n        availability: []\r\n      }\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.fetchProfile()\r\n  },\r\n\r\n  methods: {\r\n    async fetchProfile() {\r\n      this.loading = true\r\n      try {\r\n        const data = await professionalAPI.getProfile()\r\n        this.profile = data\r\n        this.form = {\r\n          description: data.description || '',\r\n          phone: data.phone || '',\r\n          experience: data.experience || '',\r\n          availability: data.availability || []\r\n        }\r\n      } catch (err) {\r\n        this.error = err.message || 'Failed to load profile'\r\n        console.error('Error fetching profile:', err)\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    },\r\n\r\n    async updateProfile() {\r\n      this.loading = true\r\n      try {\r\n        await professionalAPI.updateProfile(this.form)\r\n        this.profile = { ...this.profile, ...this.form }\r\n        this.editMode = false\r\n        this.$toast.success('Profile updated successfully')\r\n      } catch (err) {\r\n        this.$toast.error(err.message || 'Failed to update profile')\r\n        console.error('Error updating profile:', err)\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    },\r\n\r\n    cancelEdit() {\r\n      this.editMode = false\r\n      this.form = {\r\n        description: this.profile.description || '',\r\n        phone: this.profile.phone || '',\r\n        experience: this.profile.experience || '',\r\n        availability: this.profile.availability || []\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.professional-profile-page {\r\n  background-color: #f8f9fa;\r\n  min-height: 100vh;\r\n}\r\n\r\n.profile-card {\r\n  border-radius: 15px;\r\n}\r\n\r\n.profile-image {\r\n  width: 150px;\r\n  height: 150px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.profile-image img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n\r\n.rating {\r\n  display: inline-block;\r\n}\r\n\r\n.rating .fas {\r\n  color: #e0e0e0;\r\n}\r\n\r\n.rating .fas.text-warning {\r\n  color: #ffc107;\r\n}\r\n\r\n.list-group-item {\r\n  transition: background-color 0.2s;\r\n}\r\n\r\n.list-group-item:hover {\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.availability-grid {\r\n  display: flex;\r\n  gap: 10px;\r\n  overflow-x: auto;\r\n}\r\n\r\n.day-column {\r\n  flex: 1;\r\n  min-width: 100px;\r\n  text-align: center;\r\n  border: 1px solid #e0e0e0;\r\n  border-radius: 5px;\r\n  overflow: hidden;\r\n}\r\n\r\n.day-header {\r\n  background-color: #f8f9fa;\r\n  padding: 8px;\r\n  font-weight: 500;\r\n}\r\n\r\n.day-availability {\r\n  padding: 15px 8px;\r\n}\r\n\r\n.day-availability.is-available {\r\n  background-color: #d4edda;\r\n  color: #155724;\r\n}\r\n\r\n.day-availability:not(.is-available) {\r\n  background-color: #f8d7da;\r\n  color: #721c24;\r\n}\r\n</style>\r\n"],"mappings":"AAqQA,SAAAA,eAAA;AAEA;EACAC,IAAA;EAEAC,KAAA;IACA;MACAC,OAAA;MACAC,OAAA;MACAC,KAAA;MACAC,QAAA;MACAC,IAAA;QACAC,WAAA;QACAC,KAAA;QACAC,UAAA;QACAC,YAAA;MACA;IACA;EACA;EAEAC,QAAA;IACA,KAAAC,YAAA;EACA;EAEAC,OAAA;IACA,MAAAD,aAAA;MACA,KAAAT,OAAA;MACA;QACA,MAAAF,IAAA,SAAAF,eAAA,CAAAe,UAAA;QACA,KAAAZ,OAAA,GAAAD,IAAA;QACA,KAAAK,IAAA;UACAC,WAAA,EAAAN,IAAA,CAAAM,WAAA;UACAC,KAAA,EAAAP,IAAA,CAAAO,KAAA;UACAC,UAAA,EAAAR,IAAA,CAAAQ,UAAA;UACAC,YAAA,EAAAT,IAAA,CAAAS,YAAA;QACA;MACA,SAAAK,GAAA;QACA,KAAAX,KAAA,GAAAW,GAAA,CAAAC,OAAA;QACAC,OAAA,CAAAb,KAAA,4BAAAW,GAAA;MACA;QACA,KAAAZ,OAAA;MACA;IACA;IAEA,MAAAe,cAAA;MACA,KAAAf,OAAA;MACA;QACA,MAAAJ,eAAA,CAAAmB,aAAA,MAAAZ,IAAA;QACA,KAAAJ,OAAA;UAAA,QAAAA,OAAA;UAAA,QAAAI;QAAA;QACA,KAAAD,QAAA;QACA,KAAAc,MAAA,CAAAC,OAAA;MACA,SAAAL,GAAA;QACA,KAAAI,MAAA,CAAAf,KAAA,CAAAW,GAAA,CAAAC,OAAA;QACAC,OAAA,CAAAb,KAAA,4BAAAW,GAAA;MACA;QACA,KAAAZ,OAAA;MACA;IACA;IAEAkB,WAAA;MACA,KAAAhB,QAAA;MACA,KAAAC,IAAA;QACAC,WAAA,OAAAL,OAAA,CAAAK,WAAA;QACAC,KAAA,OAAAN,OAAA,CAAAM,KAAA;QACAC,UAAA,OAAAP,OAAA,CAAAO,UAAA;QACAC,YAAA,OAAAR,OAAA,CAAAQ,YAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}